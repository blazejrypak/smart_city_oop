<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane id="AnchorPane" prefHeight="608.0" prefWidth="1216.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ChangeAddress">
    <children>
        <Label layoutX="19.0" layoutY="17.0" prefHeight="29.0" prefWidth="207.0"
               style="-fx-font-size: 20; -fx-font-weight: bold; -fx-text-fill: #7387A8;" text="Change address"/>
        <Separator layoutX="3.0" layoutY="55.0" prefHeight="4.0" prefWidth="1213.0"/>
        <VBox layoutX="24.0" layoutY="77.0" prefHeight="507.0" prefWidth="683.0">
            <children>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Country:">
                            <HBox.margin>
                                <Insets right="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="23.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="id_country" prefHeight="32.0" prefWidth="247.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="City:">
                            <HBox.margin>
                                <Insets right="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="23.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="id_city" prefHeight="32.0" prefWidth="247.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Postal code:">
                            <HBox.margin>
                                <Insets right="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="23.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="id_postal_code" prefHeight="32.0" prefWidth="247.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="210.0" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Street name:">
                            <HBox.margin>
                                <Insets right="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="23.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="id_street_name" prefHeight="32.0" prefWidth="247.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="310.0" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" text="Home number:">
                            <HBox.margin>
                                <Insets right="20.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="23.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="id_home_number" prefHeight="32.0" prefWidth="247.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="410.0" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#submit"
                                prefHeight="26.0"
                                prefWidth="328.0" text="Submit">
                            <opaqueInsets>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </opaqueInsets>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
